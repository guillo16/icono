<%= simple_form_for @program do |f| %>
<p>Titulo</p>
<%= f.input :title, label: false %>
<p>Subtitulo</p>
<%= f.input :subtitle, label: false %>
<p>Contenido</p>
<%= f.input :content, label: false %>
<p>Foto</p>
<%= f.input :photos, as: :file, input_html: { multiple: true } %>
<br>
<%= f.submit 'Crear Dieta', class: 'btn btn-block btn-services' %>
<% end %>
